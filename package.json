{
  "type": "module",
  "name": "@joblocal/api-client-v5",
  "version": "1.0.6",
  "description": "This repository provides an api-client to use the version 5 joblocal api.",
  "files": [
    "dist",
    "src"
  ],
  "main": "./src/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && npm run validate",
    "build": "webpack",
    "test": "jest src",
    "test:watch": "npm test -- --watch --onlyChanged",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint . --ext js --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "validate": "npm-run-all --parallel lint test"
  },
  "author": "Joblocal GmbH",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/register": "^7.18.9",
    "babel-loader": "^8.2.5",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-webpack-plugin": "^3.2.0",
    "git-hooks": "^1.1.10",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "devour-client": "^3.1.4",
    "qs": "^6.11.0"
  }
}
